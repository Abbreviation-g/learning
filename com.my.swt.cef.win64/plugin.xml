<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="com.my.swt.cef.win64.view.CEFBrowserView"
            id="com.my.swt.cef.win64.view.example"
            name="SWT CEF View"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.my.swt.cef.win64.toolbar1">
            <command
                  commandId="com.my.swt.cef.win64.command1"
                  style="push">
            </command>
            <command
                  commandId="com.my.swt.cef.win64.command2"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.my.swt.cef.win64.command1"
            name="Command Handler1">
      </command>
      <command
            id="com.my.swt.cef.win64.command2"
            name="Command Handler2">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.my.swt.cef.win64.handler.Handler1"
            commandId="com.my.swt.cef.win64.command1">
      </handler>
      <handler
            class="com.my.swt.cef.win64.handler.Handler2"
            commandId="com.my.swt.cef.win64.command2">
            <!---->
         <enabledWhen>
            <with
                  variable="selection">
               <and>
                  <instanceof
                        value="java.lang.Object">
                  </instanceof>
                  <test
                        property="com.my.swt.cef.win64.test.handler">
                  </test>
               </and>
            </with>
         </enabledWhen>
         
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.my.swt.cef.win64.handler.HandlerPropertyTester"
            id="com.my.swt.cef.win64.propertyTester"
            namespace="com.my.swt.cef.win64.test"
            properties="handler"
            type="java.lang.Object">
      </propertyTester>
   </extension>

</plugin>
